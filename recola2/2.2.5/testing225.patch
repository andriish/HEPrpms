+program main_rcl
+  use recola
+
+  implicit none
+
+  integer, parameter    :: dp = kind (23d0)
+  integer               :: legspr(20)
+  real(dp), allocatable :: p(:,:)
+
+
+  call set_output_file_rcl ("*")
+  call set_collier_mode_rcl(1)
+  call set_dynamic_settings_rcl(1)
+  call use_gfermi_scheme_rcl(a=0.75589159739226d-02)
+
+  call set_print_level_squared_amplitude_rcl (2)
+
+  call set_alphas_rcl(0.118d0, 91d0, 5)
+
+  call set_pole_mass_tau_rcl (1.77686d0,0d0)
+  call unset_light_tau_rcl
+  call set_pole_mass_top_rcl (172.7d0,0d0)
+
+  call switchon_transverse_resonant_propagator_rcl
+  call switchoff_resonant_selfenergies_rcl
+
+  call check_uuttZ()
+  call check_ggttZ()
+  call check_ggtt()
+
+  contains
+
+  subroutine setup()
+  end subroutine
+
+  subroutine check_ggtt()
+      call setup()
+      call process_ggtt(legspr)
+      call generate_processes_rcl
+      call momenta_ggtt(p)
+      call check_uv_finiteness(p, 7)
+      call reset_recola_rcl
+  end subroutine
+
+  subroutine check_ggttZ()
+      call setup()
+      call process_ggttZ(legspr)
+      call generate_processes_rcl
+      call momenta_ggttZ(p)
+      call check_uv_finiteness(p, 10)
+      call reset_recola_rcl
+  end subroutine
+
+  subroutine check_uuttZ()
+      call setup()
+      call process_uuttZ(legspr)
+      call generate_processes_rcl
+      call momenta_uuttZ(p)
+      call check_uv_finiteness(p, 11)
+      call reset_recola_rcl
+  end subroutine
+
+  subroutine check_uv_finiteness(p, thres)
+      real(dp), intent(in) :: p(:,:)
+      integer, intent(in) :: thres
+      real(dp) :: A2(0:1), A2check(0:1), acc
+      logical :: momenta_check
+      call set_mu_uv_rcl (1d2)
+      call compute_process_rcl(1,p,'NLO',A2,momenta_check)
+      if (momenta_check .neqv. .true.) then
+        write(*,*) "Momenta check failed"
+        stop 9
+      end if
+      call set_mu_uv_rcl (1d10)
+      call compute_process_rcl(1,p,'NLO',A2check,momenta_check)
+      if (momenta_check .neqv. .true.) then
+        write(*,*) "Momenta check failed"
+        stop 9
+      end if
+      acc = -log10(abs(A2(1) / A2check(1) - 1d0))
+      if (acc < thres) then
+        write(*,*) "UV finitenes check failed", acc
+        stop 9
+      end if
+      write(*,*) A2, A2check
+  end subroutine
+
+  subroutine process_ggtt(legspr)
+    integer, intent(out) :: legspr(:)
+    call set_resonant_particle_rcl ('t')
+    call set_resonant_particle_rcl ('t~')
+    call set_resonant_particle_rcl ('W+')
+    call set_resonant_particle_rcl ('W-')
+    call define_process_rcl(1,'g g -> t(W+(nu_e e+ tau- tau+) b) t~(W-(mu- nu_mu~) b~)','NLO'); legspr(1) = 10
+    ! call define_process_rcl(1,'g g -> nu_e e+ tau- tau+ b mu- nu_mu~ b~','NLO'); legspr(1) = 10
+    call define_process_rcl(2,'g g -> t t~','LO'); legspr(2) = 4
+    call define_process_rcl(3,'t -> W+ b','LO'); legspr(3) = 3
+    call define_process_rcl(4,'t~ -> W- b~','LO'); legspr(4) = 3
+    call define_process_rcl(5,'W+ -> nu_e e+ tau- tau+','LO'); legspr(5) = 5
+    call define_process_rcl(6,'W- -> mu- nu_mu~','LO'); legspr(6) = 3
+    call unselect_all_gs_powers_LoopAmpl_rcl(1)
+    call select_gs_power_LoopAmpl_rcl(1,2)
+  end subroutine
+
+  subroutine process_ggttZ(legspr)
+    integer, intent(out) :: legspr(:)
+    call define_process_rcl(1,'g g -> t t~ Z(tau- tau+)','NLO'); legspr(1) = 6
+    call set_resonant_particle_rcl ('Z')
+    call define_process_rcl(2,'g g -> t t~ Z','LO'); legspr(2) = 5
+    call define_process_rcl(3,'Z -> tau- tau+','LO'); legspr(3) = 3
+
+    call unselect_all_gs_powers_LoopAmpl_rcl(1)
+    call select_gs_power_LoopAmpl_rcl(1,2)
+  end
+
+  subroutine process_uuttZ(legspr)
+    integer, intent(out) :: legspr(:)
+    call set_resonant_particle_rcl ('Z')
+    call define_process_rcl(1,'u u~ -> t t~ Z(tau- tau+)','NLO'); legspr(1) = 6
+    call define_process_rcl(2,'u u~ -> t t~ Z','LO'); legspr(2) = 5
+    call define_process_rcl(3,'Z -> tau- tau+','LO'); legspr(3) = 3
+
+    call unselect_all_gs_powers_BornAmpl_rcl(1)
+    call select_gs_power_BornAmpl_rcl(1,2)
+    call unselect_all_gs_powers_LoopAmpl_rcl(1)
+    call select_gs_power_LoopAmpl_rcl(1,2)
+  end subroutine
+
+  subroutine momenta_ggtt(p)
+    real(dp), allocatable, intent(out) :: p(:,:)
+    real(dp) :: ppp(0:3,1:10,1:10)
+    real(dp) :: roots
+    integer :: j
+
+    allocate(p(0:3, 10))
+
+    roots = 4000d0
+  
+    p(0,1) = + roots/2d0
+    p(1,1) =   0d0
+    p(2,1) =   0d0
+    p(3,1) = + roots/2d0
+    p(0,2) = + roots/2d0
+    p(1,2) =   0d0
+    p(2,2) =   0d0
+    p(3,2) = - roots/2d0
+    call set_outgoing_momenta_rcl(2,p(0:3,1:2),p(0:3,1:legspr(2)))
+  
+    ppp(0:3,1,3) = p(0:3,3)
+    ppp(0:3,1,4) = p(0:3,4)
+    do j = 3,4
+      call set_outgoing_momenta_rcl(j,ppp(0:3,1:1,j),ppp(0:3,1:legspr(j),j))
+    enddo
+  
+    ppp(0:3,1,5) = ppp(0:3,2,3)
+    ppp(0:3,1,6) = ppp(0:3,2,4)
+    do j = 5,6
+      call set_outgoing_momenta_rcl(j,ppp(0:3,1:1,j),ppp(0:3,1:legspr(j),j))
+    enddo
+  
+    p(0:3, 3) = ppp(0:3,2,5)
+    p(0:3, 4) = ppp(0:3,3,5)
+    p(0:3, 5) = ppp(0:3,4,5)
+    p(0:3, 6) = ppp(0:3,5,5)
+  
+    p(0:3, 7) = ppp(0:3,3,3)
+  
+    p(0:3, 8) = ppp(0:3,2,6)
+    p(0:3, 9) = ppp(0:3,3,6)
+  
+    p(0:3,10) = ppp(0:3,3,4)
+  
+    call set_resonant_squared_momentum_rcl (1,1,175d0**2)
+    call set_resonant_squared_momentum_rcl (1,2,85d0**2)
+    call set_resonant_squared_momentum_rcl (1,3,180d0**2)
+    call set_resonant_squared_momentum_rcl (1,4,90d0**2)
+  end subroutine 
+
+
+  subroutine momenta_ggttZ(p)
+    real(dp), allocatable, intent(out) :: p(:,:)
+    real(dp) :: pp(0:3,1:10)
+    real(dp) :: roots
+
+    allocate(p(0:3, 6))
+
+    roots = 4000d0
+  
+    p(0,1) = + roots/2d0
+    p(1,1) =   0d0
+    p(2,1) =   0d0
+    p(3,1) = + roots/2d0
+    p(0,2) = + roots/2d0
+    p(1,2) =   0d0
+    p(2,2) =   0d0
+    p(3,2) = - roots/2d0
+    call set_outgoing_momenta_rcl(2,p(0:3,1:2),p(0:3,1:legspr(2)))
+  
+    pp(0:3,1) = p(0:3,5)
+    call set_outgoing_momenta_rcl(3,pp(0:3,1:1),pp(0:3,1:legspr(3)))
+  
+    p(0:3,5) = pp(0:3,2)
+    p(0:3,6) = pp(0:3,3)
+    call set_resonant_squared_momentum_rcl (1,1,92d0**2)
+  end subroutine
+
