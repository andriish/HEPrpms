+  write(*,*) "ME2 = ", M2prod*M2dec1*M2dec2/(MW*wwidth)**4
+
+  if (abs(M2prod*M2dec1*M2dec2/(MW*wwidth)**4 - M2full)/M2full < 1d-12) then
+     write(*,*) "Internal projection successful!"
+  else
+     write(*,*) "Internal projection failed!"
+     stop 9
+  end if
+
+  call reset_recola_rcl
+end program internal_projection
