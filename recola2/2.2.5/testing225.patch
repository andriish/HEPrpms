+!------------------------------------------------------------------------------!
+
+  subroutine helicity_projection(prj,pa,m2,p,pl,w0out)
+
+    integer,          intent(in)   :: prj,pa
+    complex(dp),      intent(in)   :: m2,p(:),pl(:)
+    complex(dp),      intent(inout) :: w0out(0:3)
+
+    complex(dp) :: pol1(0:3)
+    complex(dp) :: pol1_cv(0:3),pol1a(0:3),pol1a_cv(0:3),pol1b(0:3),pol1b_cv(0:3)
+    complex(dp) :: contr_tmp(0:3),contr_tmpa(0:3),contr_tmpb(0:3)
+    real(dp)    :: mass_proj
+    logical     :: is_aux
+
+    if ((prj .ge. -1) .and. (prj .le. 1) .and. &
+         get_particle_type2_mdl(pa) .eq. 'v') then
+
+      mass_proj = sqrt(real(m2,kind=dp))
+      call definewp(pa, p, pl, mass_proj, prj, pol1)
+      pol1_cv(0) = pol1(0)
+      pol1_cv(1:3) = -pol1(1:3)
+      contr_tmp = sum(w0out(:)*pol1_cv(:))*conjg(pol1(:))
+      w0out(:) = -contr_tmp
+    end if
+
+    if (prj .eq. 3 .and. get_particle_type2_mdl(pa) .eq. 'v') then
+        mass_proj = sqrt(real(m2,kind=dp))
+        call definewp(pa, p, pl, mass_proj,-1, pol1a)
+        pol1a_cv(0) = pol1a(0)
+        pol1a_cv(1:3) = -pol1a(1:3)
+        contr_tmpa = sum(w0out(:)*pol1a_cv(:))*conjg(pol1a(:))
+        call definewp(pa, p, pl, mass_proj,+1, pol1b) ! right pol vec
+        pol1b_cv(0) = pol1b(0)
+        pol1b_cv(1:3) = -pol1b(1:3)
+        contr_tmpb = sum(w0out(:)*pol1b_cv(:))*conjg(pol1b(:))
+        w0out(:) = -contr_tmpa-contr_tmpb
+     end if
+
+     if ( (abs(prj).eq.1 .or. prj .eq. 3) &
+         .and. &
+         ((get_particle_type_mdl(pa) .eq. 'f') .or. &
+          (get_particle_type_mdl(pa) .eq. 'f~')) &
+      ) then
+         call error_rcl('Helicity projection not implemented for fermions.', &
+                        where='helicity_projection')
+     end if
+
+     is_aux = ( &
+        (get_particle_type_mdl(pa) .eq. 'x') .or. &
+        (get_particle_type_mdl(pa) .eq. 'x~') .or. &
+        (get_particle_type_mdl(pa) .eq. 's') &
+     )
+     ! Ghosts & Goldstone boson - transverse
+     if (drop_goldstone_transverse .and. ((prj .eq. -1) .or. (prj .eq. 1)) .and. is_aux ) then
+       w0out(:) = 0
+     end if
+     ! Ghosts & Goldstone boson - coherent sum
+     if (drop_goldstone_transverse .and. (prj .eq. 3) .and. is_aux ) then
+       w0out(:) = 0
+     end if
+     ! Ghosts & Goldstone boson - longitudinal
+     if (drop_goldstone_longitudinal .and. (prj .eq. 0) .and. is_aux ) then
+       w0out(:) = 0
+     end if
+
+  end subroutine helicity_projection
+
 !------------------------------------------------------------------------------!
 
 end module amplitude_rcl
