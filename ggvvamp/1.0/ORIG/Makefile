all: evalcmdlinegg

libggvvamp.so: $(OBJ)
	$(CXX) -o $@ $+ $(SOFLAGS)

libggvvamp.a: $(OBJ)
	ar rs $@ $+

evalcmdlinegg: evalcmdlinegg.o libggvvamp.so
	$(CXX) -o $@ $< -L. -lggvvamp $(LDFLAGS) 

evalcmdlinegg-static: evalcmdlinegg.o libggvvamp.a
	$(CXX) -o $@ $+ $(LDFLAGS) 

%.o: %.cpp 
	$(CXX) -c -o $@ $< $(CXXFLAGS) 

clean:
	rm -f libggvvamp.so libggvvamp.a evalcmdlinegg evalcmdlinegg-static *.o *~

.PHONY: clean
